 tempCart[index];\r\n\r\n        product.count = product.count + 1;\r\n        product.total = product.count * product.price;\r\n        this.setState(()=>{\r\n            return { GiftCart: [...tempCart]}\r\n        },()=> {\r\n            this.makeTotal();\r\n        }\r\n\r\n        )\r\n    }\r\n\r\n\r\n    decrement=(id)=>{\r\n        let tempCart=[...this.state.GiftCart];\r\n        const selectedProduct = tempCart.find(item=> item.id === id);\r\n        const index= tempCart.indexOf(selectedProduct);\r\n        const product = tempCart[index];\r\n\r\n        product.count = product.count - 1;\r\n        product.total = product.count * product.price;\r\n        this.setState(()=>{\r\n            return { GiftCart: [...tempCart]}\r\n        },()=> {\r\n            this.makeTotal();\r\n        }\r\n\r\n        )\r\n    }\r\n\r\n    removeItem = (id)=> {\r\n        let tempProduct = [...this.state.products];\r\n        let tempCart = [...this.state.GiftCart];\r\n        tempCart = tempCart.filter(item=> item.id !== id);\r\n        const index = tempProduct.indexOf(this.getItem(id));\r\n\r\n        let removedProd = tempProduct[index];\r\n\r\n        removedProd.inCart = false;\r\n        removedProd.total = 0;\r\n        removedProd.count = 0;\r\n\r\n        this.setState(()=> {\r\n            return{\r\n                GiftCart : [...tempCart],\r\n                product : [...tempProduct]\r\n            }\r\n        }, \r\n        ()=>{\r\n            return this.makeTotal();\r\n        }\r\n        )\r\n\r\n    }\r\n\r\n    makeTotal = () => {\r\n        let subTotal = 0;\r\n        this.state.GiftCart.map(item => (subTotal += item.total));\r\n        const total = subTotal;\r\n        this.setState(()=>{\r\n            return{\r\n                GiftCartSubTotal : subTotal\r\n            }\r\n        })\r\n\r\n\r\n    }\r\n\r\n    render(){\r\n        return(\r\n            <ProductContext.Provider value={{\r\n                ...this.state,\r\n               